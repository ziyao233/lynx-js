LD_RPATH_OPT=
if test "x$cf_cv_enable_rpath" != xno
then
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for an rpath option" >&5
printf %s "checking for an rpath option... " >&6; }
	case "$cf_cv_system_name" in
	(irix*)
		if test "$GCC" = yes; then
			LD_RPATH_OPT="-Wl,-rpath,"
		else
			LD_RPATH_OPT="-rpath "
		fi
		;;
	(linux*|gnu*|k*bsd*-gnu|freebsd*)
		LD_RPATH_OPT="-Wl,-rpath,"
		;;
	(openbsd[2-9].*|mirbsd*)
		LD_RPATH_OPT="-Wl,-rpath,"
		;;
	(dragonfly*)
		LD_RPATH_OPT="-rpath "
		;;
	(netbsd*)
		LD_RPATH_OPT="-Wl,-rpath,"
		;;
	(osf*|mls+*)
		LD_RPATH_OPT="-rpath "
		;;
	(solaris2*)
		LD_RPATH_OPT="-R"
		;;
	(*)
		;;
	esac
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $LD_RPATH_OPT" >&5
printf "%s\n" "$LD_RPATH_OPT" >&6; }

	case "x$LD_RPATH_OPT" in
	(x-R*)
